# .env file for LOCAL DEVELOPMENT ONLY
#
# For Docker: Pass these as environment variables with docker run -e
# See README.md for Docker usage
#
# For local Python development: Copy this to .env and customize

# DuckDB Configuration
# For local testing, use absolute path or relative to uploader directory
# Example: ../mqtt_logs.db or /Users/yourname/path/to/mqtt_logs.db
DUCKDB_PATH=../mqtt_logs.db
MINUTES_OF_DATA=10

# S3 Configuration (pre-configured)
S3_BUCKET=treelemetry-sbma44-water-data
S3_KEY=water-level.json

# AWS Credentials (REQUIRED - get from CDK deployment output)
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Replay Configuration
REPLAY_DELAY_SECONDS=300

# Upload Interval (for daemon mode)
UPLOAD_INTERVAL_SECONDS=30


